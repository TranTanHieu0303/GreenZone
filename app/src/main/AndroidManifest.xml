<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.greenzone">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo_2"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/Theme.GreenZone">
        <activity android:name=".Activity.GroupActivity"></activity>
        <activity android:name=".Activity.HomeActivity" />
        <activity android:name=".Activity.DangKyActivity" />
        <activity android:name=".Activity.XacThucActivity" />
        <activity android:name=".Activity.LoginActivity" />
        <activity android:name=".Activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>